      <h2><span class="glyphicon glyphicon-picture" aria-hidden="true"></span> <%= @user.first_name + " " + @user.last_name %></h2>
      <hr>
      <h2><small>@<%= @user.username %></small></h2>
      <br>
      <a ng-if="$ctrl.userProfile.link.includes('unfollow')" href="{{ $ctrl.userProfile.link }}" class="btn transparentButton btn-default center-block" style="margin-top:8px;margin-bottom:8px;" role="button">Unfollow</a>
      <a ng-else   href="{{ $ctrl.userProfile.link }}" class="btn transparentButton btn-default center-block" style="margin-top:8px;margin-bottom:8px;" role="button">Follow</a>
</div>

<div class="row" ng-repeat="post in $ctrl.userProfile.posts">
  <div class="col-sm-12 col-md-12 col-lg-12">
    <div class="thumbnail">
      <div class="caption">
          <h3><span style="color:orange;"><span class="glyphicon glyphicon-picture" aria-hidden="true"></span> {{ post.image_heading }}</span><hr> {{ post.image_caption }}</h3>
          <p><a href="#" class="btn btn-default disabledTransparentButton disabled"  role="button"><span class="glyphicon glyphicon-time" aria-hidden="true"></span> Posted {{ post.time }} ago</a>
      </div>
      <img src="{{ post.url }}" alt="<%= post.image_caption %>">
    </div>
  </div>
</div>
